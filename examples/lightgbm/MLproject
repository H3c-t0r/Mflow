name: lightgbm-example
conda_env: conda.yaml
entry_points:
  main:
    parameters:
      colsample_bytree: {type: float, default: 1.0}
      subsample: {type: float, default: 1.0}
    command: |
        python train.py \
          --colsample_bytree={colsample_bytree} \
          --subsample={subsample}
