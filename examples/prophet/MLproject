name: tutorial

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      csv_url: {type: uri, default: 'https://raw.githubusercontent.com/facebook/prophet/master/examples/example_wp_log_peyton_manning.csv'}
      rolling_window: {type: float, default: 0.1}
    command: "python train.py {csv_url} {rolling_window}"
